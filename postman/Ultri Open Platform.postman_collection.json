									"});",
									"",
									"",
									"",
									"// Set variables for deviceId and preAuthSessionId so we can use them when submitting the code",
									"",
									"pm.environment.set(\"userEmail_1\", jsonData.user.email);",
									"pm.environment.set(\"userId_1\", jsonData.user.id);",
									"pm.environment.set(\"userTimeJoined_1\", jsonData.user.timeJoined);",
									""
								],
								"type": "text/javascript"
							}
